   <main role="main" class="inner cover w-100 my-auto">
        <h1 class="cover-heading text-center font-color">Women Who Code News</h1>
        <div class="row">
            <% @tweets.each do |tweet| %>
            <div class="col-md-6">
                <div class="tech-news">
                    <div class="card">
                        <%= twitter_parser tweet %>
                    </div>
                </div>
            </div>
            <% end %>
        </div>
    </main>
